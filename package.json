{
  "name": "plex-landing",
  "version": "0.0.1",
  "description": "Landing page for plex",
  "main": "./dist/server/index",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jordond/plex-landing.git"
  },
  "author": "Jordon de Hoog",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordond/plex-landing/issues"
  },
  "homepage": "https://github.com/jordond/plex-landing#readme",
  "scripts": {
    "test": "better-npm-run test",
    "test-watch": "npm test -- --auto-watch --no-single-run",
    "lint": "eslint -c .eslintrc src",
    "start": "better-npm-run start:prod",
    "start-dev": "better-npm-run start:dev",
    "build": "npm run lint && npm run build-server && better-npm-run build:prod",
    "build-watch": "npm run lint && better-npm-run build:prod:watch",
    "build-dev": "better-npm-run build:dev",
    "build-server": "webpack --config webpack.server.js",
    "dev": "concurrently --kill-others \"npm run build-dev\" \"npm run dev-server\"",
    "dev-test": "concurrently --kill-others \"npm run dev\" \"npm run test-watch\"",
    "dev-prod": "concurrently --kill-others \"npm run build-watch\" \"npm run start\"",
    "hook-install": "precommit install",
    "hook-remove": "precommit remove",
    "upgrade": "git pull && npm install",
    "dev-server": "better-npm-run nodemon:dev"
  },
  "precommit": [
    "npm run lint"
  ],
  "betterScripts": {
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "start:prod": {
      "command": "node ./dist/server",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "./src",
        "PORT": 8080
      }
    },
    "start:dev": {
      "command": "node ./dist/server",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./src",
        "PORT": 9000
      }
    },
    "build:prod": {
      "command": "webpack --verbose --colors --display-error-details --config webpack.client.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:prod:watch": {
      "command": "webpack --watch --debug --verbose --colors --display-error-details --config webpack.client.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "webpack --watch --debug --colors --display-error-details --config webpack.client.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 9000
      }
    },
    "nodemon:dev": {
      "command": "nodemon --watch ./src/server --delay 250ms --exec 'npm run build-server; npm run start-dev' --debug",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./src",
        "PORT": 9000
      }
    },
    "nodemon:prod": {
      "command": "node ./dist/server",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "./src",
        "PORT": 8080
      }
    }
  },
  "dependencies": {
    "bcrypt": "^0.8.6",
    "bluebird": "^3.4.0",
    "chalk": "^1.1.3",
    "hat": "0.0.3",
    "jsonfile": "^2.3.1",
    "jsonwebtoken": "^7.0.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-favicon": "^2.0.0",
    "koa-helmet": "^1.0.0",
    "koa-morgan": "^1.0.1",
    "koa-passport": "^2.0.1",
    "koa-router": "^7.0.1",
    "koa-session": "^3.3.1",
    "koa-static": "^2.0.0",
    "lodash": "^4.12.0",
    "mailcomposer": "^3.9.0",
    "mailgun-js": "^0.7.11",
    "mkdirp-promise": "^2.0.0",
    "moment": "^2.13.0",
    "passport-local": "^1.0.0",
    "pretty-error": "^2.0.0",
    "sequelize": "^3.23.2",
    "socket.io": "^1.4.6",
    "sqlite3": "^3.1.4",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "angular": "^1.5.5",
    "angular-animate": "^1.5.5",
    "angular-aria": "^1.5.5",
    "angular-material": "^1.0.8",
    "angular-material-icons": "^0.7.0",
    "angular-messages": "^1.5.5",
    "angular-mocks": "^1.5.5",
    "angular-moment": "^1.0.0-beta.6",
    "angular-route": "^1.5.5",
    "angular-sanitize": "^1.5.5",
    "angular-socket-io": "^0.7.0",
    "angular-ui-router": "^0.3.0",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.0",
    "better-npm-run": "0.0.8",
    "browser-sync": "^2.12.8",
    "browser-sync-webpack-plugin": "^1.0.1",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "concurrently": "^2.1.0",
    "core-js": "^2.4.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "ng-annotate": "^1.2.1",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "normalize.css": "^4.1.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "precommit": "^1.2.0",
    "sass-loader": "^3.2.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-node-externals": "^1.2.0"
  }
}
